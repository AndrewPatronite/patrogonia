    "@types/react-typist": "^2.0.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "2.0.4",
    "prettier-eslint": "^16.3.0",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/serviceWorker.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
